{
    "configId": "advanced-table-config",
    "version": "1.0",
    "structure": {
        "sections": [
            {
                "id": "personal",
                "name": "Personal Information",
                "order": 0,
                "categories": [
                    "basic",
                    "contact"
                ]
            },
            {
                "id": "professional",
                "name": "Professional Details",
                "order": 1,
                "categories": [
                    "work",
                    "skills"
                ]
            },
            {
                "id": "financial",
                "name": "Financial Information",
                "order": 2,
                "categories": [
                    "income",
                    "expenses"
                ]
            }
        ],
        "categories": [
            {
                "id": "basic",
                "name": "Basic Details",
                "sectionId": "personal",
                "order": 0,
                "subCategories": [
                    "personalBasic"
                ]
            },
            {
                "id": "contact",
                "name": "Contact Information",
                "sectionId": "personal",
                "order": 1,
                "subCategories": [
                    "contactDetails"
                ]
            },
            {
                "id": "work",
                "name": "Work Experience",
                "sectionId": "professional",
                "order": 0,
                "subCategories": [
                    "currentWork",
                    "previousWork"
                ]
            },
            {
                "id": "skills",
                "name": "Skills & Certifications",
                "sectionId": "professional",
                "order": 1,
                "subCategories": [
                    "technicalSkills",
                    "certifications"
                ]
            },
            {
                "id": "income",
                "name": "Income Sources",
                "sectionId": "financial",
                "order": 0,
                "subCategories": [
                    "salary",
                    "investments"
                ]
            },
            {
                "id": "expenses",
                "name": "Monthly Expenses",
                "sectionId": "financial",
                "order": 1,
                "subCategories": [
                    "fixed",
                    "variable"
                ]
            }
        ],
        "subCategories": [
            {
                "id": "personalBasic",
                "name": "Personal Basic Info",
                "categoryId": "basic",
                "order": 0,
                "columns": [
                    "name",
                    "dob",
                    "gender"
                ]
            },
            {
                "id": "contactDetails",
                "name": "Contact Details",
                "categoryId": "contact",
                "order": 0,
                "columns": [
                    "email",
                    "phone",
                    "address"
                ]
            },
            {
                "id": "currentWork",
                "name": "Current Employment",
                "categoryId": "work",
                "order": 0,
                "columns": [
                    "company",
                    "position",
                    "startDate"
                ]
            },
            {
                "id": "technicalSkills",
                "name": "Technical Skills",
                "categoryId": "skills",
                "order": 0,
                "columns": [
                    "skillName",
                    "proficiency",
                    "yearsOfExperience"
                ]
            },
            {
                "id": "salary",
                "name": "Salary Details",
                "categoryId": "income",
                "order": 0,
                "columns": [
                    "baseSalary",
                    "bonus",
                    "deductions"
                ]
            },
            {
                "id": "fixed",
                "name": "Fixed Expenses",
                "categoryId": "expenses",
                "order": 0,
                "columns": [
                    "rent",
                    "utilities",
                    "insurance"
                ]
            }
        ]
    },
    "columns": [
        {
            "id": "name",
            "field": "name",
            "label": "Full Name",
            "type": "text",
            "validation": {
                "required": true,
                "minLength": 2
            }
        },
        {
            "id": "dob",
            "field": "dateOfBirth",
            "label": "Date of Birth",
            "type": "date",
            "validation": {
                "required": true
            }
        },
        {
            "id": "email",
            "field": "email",
            "label": "Email Address",
            "type": "email",
            "validation": {
                "required": true,
                "format": "email"
            }
        },
        {
            "id": "baseSalary",
            "field": "baseSalary",
            "label": "Base Salary",
            "type": "number",
            "validation": {
                "required": true,
                "min": 0
            },
            "format": {
                "type": "currency",
                "options": {
                    "currency": "USD",
                    "minimumFractionDigits": 2
                }
            }
        }
    ],
    "calculations": [
        {
            "id": "totalIncome",
            "type": "sum",
            "label": "Total Income",
            "dependencies": [
                "baseSalary",
                "bonus"
            ],
            "formula": "baseSalary + bonus"
        },
        {
            "id": "netIncome",
            "type": "custom",
            "label": "Net Income",
            "dependencies": [
                "totalIncome",
                "deductions"
            ],
            "formula": "totalIncome - deductions"
        }
    ],
    "display": {
        "theme": {
            "colors": {
                "primary": "#2563eb",
                "secondary": "#6b7280",
                "background": "#ffffff"
            },
            "typography": {
                "fontFamily": "Inter, sans-serif",
                "fontSize": "14px"
            }
        },
        "layout": {
            "stickyHeader": true,
            "dense": false,
            "maxHeight": "600px"
        }
    }
}